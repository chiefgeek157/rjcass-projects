<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="snippetFactory"
		class="com.rjcass.xbml.m2analyzer.model.SnippetFactory" />

	<bean id="entityFactory"
		class="com.rjcass.xbml.m2analyzer.model.XBMLEntityFactory" />

	<bean id="modelBuilderFactory"
		class="com.rjcass.xbml.m2analyzer.model.M2ModelBuilderFactoryImpl">
		<property name="snippetFactory" ref="snippetFactory" />
		<property name="entityFactory" ref="entityFactory" />
	</bean>

</beans>
